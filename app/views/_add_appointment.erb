<div class="appt-new" style='display: none;'>
  <h3>next time block</h3>
  <label for="date">date</label>
   <input id="date" type="date" name="timeblock[][date]"/>
   <label for="start_time">start time</label>
   <input id="start_time" type="time" name="timeblock[][start_at]"/>
   <label for="end_time">end time</label>
   <input id="end_time" type="time" name="timeblock[][end_at]"/>
   <label for="duration">duration</label>
   <input id="duration" type="number" name="timeblock[][duration]" min="5" max="120" step="5" value='10'>
</div>
